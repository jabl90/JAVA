
public class Array {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
	 String[] meses = new String[12]; //estamos creando un objeto array
	 
	 meses[0]="Enero";
	 meses[1]="Febrero";
	 meses[2]="Marzo";
	 meses[3]="Abril";
	 meses[4]="Mayo";
	 meses[5]="Junio";
	 meses[6]="Julio";
	 meses[7]="Agosto";
	 meses[8]="Septiembre";
	 meses[9]="Octubre";
	 meses[10]="Noviembre";
	 meses[11]="Dicembre";
	 
	 for (int i = 0; i < meses.length; i++) {
		System.out.println("El mes "+(i+1)+"es"+meses[i]);
	}
	 
	}

}
